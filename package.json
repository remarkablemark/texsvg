{
  "name": "texsvg",
  "version": "2.2.1",
  "description": "Converts TeX to SVG.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "./cjs/index.js",
  "bin": {
    "texsvg": "./cjs/bin.js"
  },
  "scripts": {
    "build": "tsc --outDir cjs",
    "clean": "rm -rf cjs",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run lint:tsc && npm test && npm run clean && npm run build",
    "test": "jest --colors",
    "test:ci": "CI=true npm test -- --coverage --ci",
    "test:integration": "npm run build && npm test -- --config=jest.integration.ts",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/texsvg"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/texsvg/issues"
  },
  "keywords": [
    "tex",
    "svg",
    "latex",
    "math",
    "equation",
    "formula"
  ],
  "dependencies": {
    "mathjax": "3.2.2",
    "svgo": "3.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.12",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "eslint": "8.57.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.1",
    "jest": "29.7.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "ts-jest": "29.2.3",
    "typescript": "5.5.4"
  },
  "files": [
    "cjs/"
  ],
  "engines": {
    "node": ">=14"
  },
  "license": "MIT"
}
