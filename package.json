{
  "name": "texsvg",
  "version": "3.0.0",
  "description": "Converts TeX to SVG.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "./cjs/index.js",
  "bin": {
    "texsvg": "cjs/bin.js"
  },
  "scripts": {
    "build": "tsc --outDir cjs",
    "clean": "rm -rf cjs src/coverage",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run lint:tsc && npm test && npm run clean && npm run build",
    "test": "jest --colors",
    "test:ci": "CI=true npm test -- --coverage --ci",
    "test:integration": "npm run build && node --test **/*.test.mjs && npm test -- --config=jest.config.integration.ts",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remarkablemark/texsvg.git"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/texsvg/issues"
  },
  "keywords": [
    "tex",
    "svg",
    "latex",
    "math",
    "equation",
    "formula"
  ],
  "dependencies": {
    "mathjax": "3.2.2",
    "svgo": "4.0.0",
    "yargs": "17"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/compat": "1.3.2",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.35.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.3",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "eslint": "9.35.0",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "lint-staged": "16.1.6",
    "prettier": "3.6.2",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "files": [
    "cjs/"
  ],
  "engines": {
    "node": ">=14"
  },
  "license": "MIT"
}
