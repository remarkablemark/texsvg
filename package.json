{
  "name": "texsvg",
  "version": "1.3.0",
  "description": "Converts TeX to SVG.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "cjs/index.js",
  "bin": {
    "texsvg": "./cjs/bin.js"
  },
  "scripts": {
    "build": "tsc --outDir cjs",
    "clean": "rm -rf cjs",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable && npm run lint && npm run lint:tsc && npm test && npm run clean && npm run build",
    "release": "standard-version --no-verify",
    "test": "jest",
    "test:ci": "jest --coverage --ci",
    "test:integration": "npm run build && jest --config=jest.integration.js",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/texsvg"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/texsvg/issues"
  },
  "keywords": [
    "tex",
    "svg",
    "latex",
    "math",
    "equation",
    "formula"
  ],
  "dependencies": {
    "@types/svgo": "1.3.3",
    "mathjax": "3.1.0",
    "svgo": "1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "standard-version": "^9.2.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "files": [
    "cjs/"
  ],
  "engines": {
    "node": ">=4"
  },
  "license": "MIT"
}
