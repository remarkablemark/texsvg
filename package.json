{
  "name": "texsvg",
  "version": "2.0.4",
  "description": "Converts TeX to SVG.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "./cjs/index.js",
  "bin": {
    "texsvg": "./cjs/bin.js"
  },
  "types": "./cjs/index.d.ts",
  "scripts": {
    "build": "tsc --outDir cjs",
    "clean": "rm -rf cjs",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable && npm run lint && npm run lint:tsc && npm test && npm run clean && npm run build",
    "test": "jest --colors",
    "test:ci": "jest --coverage --ci",
    "test:integration": "npm run build && jest --config=jest.integration.ts",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/texsvg"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/texsvg/issues"
  },
  "keywords": [
    "tex",
    "svg",
    "latex",
    "math",
    "equation",
    "formula"
  ],
  "dependencies": {
    "mathjax": "3.2.2",
    "svgo": "3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@types/jest": "29.5.10",
    "@types/node": "20.9.4",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "eslint": "8.54.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.1.0",
    "pinst": "3.0.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "typescript": "5.3.2"
  },
  "files": [
    "cjs/"
  ],
  "engines": {
    "node": ">=14"
  },
  "license": "MIT"
}
