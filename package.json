{
  "name": "texsvg",
  "version": "1.5.0",
  "description": "Converts TeX to SVG.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "cjs/index.js",
  "bin": {
    "texsvg": "./cjs/bin.js"
  },
  "scripts": {
    "build": "tsc --outDir cjs",
    "clean": "rm -rf cjs",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable && npm run lint && npm run lint:tsc && npm test && npm run clean && npm run build",
    "test": "jest --colors",
    "test:ci": "jest --coverage --ci",
    "test:integration": "npm run build && jest --config=jest.integration.ts",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/texsvg"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/texsvg/issues"
  },
  "keywords": [
    "tex",
    "svg",
    "latex",
    "math",
    "equation",
    "formula"
  ],
  "dependencies": {
    "@types/svgo": "2.6.3",
    "mathjax": "3.2.2",
    "svgo": "2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "files": [
    "cjs/"
  ],
  "engines": {
    "node": ">=4"
  },
  "license": "MIT"
}
