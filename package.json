{
  "name": "texsvg",
  "version": "1.5.1",
  "description": "Converts TeX to SVG.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "cjs/index.js",
  "bin": {
    "texsvg": "./cjs/bin.js"
  },
  "scripts": {
    "build": "tsc --outDir cjs",
    "clean": "rm -rf cjs",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable && npm run lint && npm run lint:tsc && npm test && npm run clean && npm run build",
    "test": "jest --colors",
    "test:ci": "jest --coverage --ci",
    "test:integration": "npm run build && jest --config=jest.integration.ts",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/texsvg"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/texsvg/issues"
  },
  "keywords": [
    "tex",
    "svg",
    "latex",
    "math",
    "equation",
    "formula"
  ],
  "dependencies": {
    "@types/svgo": "2.6.4",
    "mathjax": "3.2.2",
    "svgo": "2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "8.27.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.0.3",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "files": [
    "cjs/"
  ],
  "engines": {
    "node": ">=4"
  },
  "license": "MIT"
}
